<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 400">
  <!-- Black Background -->
  <rect width="1200" height="400" fill="#000000"/>
  
  <!-- Background Texture Pattern -->
  <defs>
    <pattern id="dots" patternUnits="userSpaceOnUse" width="20" height="20" patternTransform="rotate(45)">
      <circle cx="10" cy="10" r="1.5" fill="#333333"/>
    </pattern>
    <pattern id="lines" patternUnits="userSpaceOnUse" width="50" height="50">
      <path d="M0,0 l50,50" stroke="#222222" stroke-width="1"/>
      <path d="M50,0 l-50,50" stroke="#222222" stroke-width="1"/>
    </pattern>
  </defs>
  
  <!-- Textured Background Elements -->
  <rect width="1200" height="400" fill="url(#dots)" opacity="0.5"/>
  <rect width="1200" height="400" fill="url(#lines)" opacity="0.3"/>
  
  <!-- Fun Geometric Elements - Circle moved further right -->
  <circle cx="1100" cy="80" r="60" fill="#F7A81B" opacity="0.4"/>
  <rect x="1000" y="270" width="120" height="120" fill="#1E1E5A" opacity="0.4"/>
  <polygon points="850,320 900,280 950,320" fill="#F7A81B" opacity="0.3"/>
  
  <!-- Logo placement -->
  <image href="/api/placeholder/300/300" x="50" y="50" width="300" height="300" preserveAspectRatio="xMidYMid meet"/>
  
  <!-- Main Text: Upcoming Events! -->
  <text x="420" y="170" font-family="Arial, sans-serif" font-size="70" font-weight="bold" fill="#ffffff">UPCOMING EVENTS!</text>
  
  <!-- Secondary Text: Updated as requested -->
  <text x="420" y="250" font-family="Arial, sans-serif" font-size="40" fill="#F7A81B">PRESENTED BY AGBU YPGNY</text>
  
  <!-- Decorative Element -->
  <rect x="420" y="190" width="700" height="4" fill="#F7A81B"/>
</svg>